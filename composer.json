{
    "name": "ryancco/laravel-sessions",
    "description": "The missing model for Laravel sessions",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Colson",
            "email": "ryan@ryanc.co"
        }
    ],
    
    "require": {
        "php": "^7.2",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "orchestra/testbench": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Ryancco\\Sessions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ryancco\\Sessions\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ryancco\\Sessions\\SessionsServiceProvider"
            ]
        }
    }
}
